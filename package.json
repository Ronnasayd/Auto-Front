{
  "name": "autofront",
  "description": "Help in front-end development",
  "version": "0.0.3",
  "main": "gulpfile.js",
  "author": "Ronnasayd de Sousa Machado",
  "license": "MIT",
  "homepage": "https://github.com/Ronnasayd/Auto-Front/",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ronnasayd/Auto-Front.git"
  },
  "bugs": {
    "url": "https://github.com/Ronnasayd/Auto-Front/issues"
  },
  "scripts": {
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "commitizen_init": "commitizen init cz-conventional-changelog --yarn --dev --exact --force",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true && devmoji -e --lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "app/src/**/*.js": [
      "eslint --fix"
    ],
    "app/src/**/*.scss": [
      "stylelint --fix"
    ],
    "app/src/**/*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "^2.1.9",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-partials-imported": "^1.0.7",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.12.0",
    "prettier": "^2.1.2",
    "snyk": "^1.162.0",
    "standard-version": "^8.0.1",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "webpack-stream": "^5.2.1"
  },
  "browserslist": [
    "last 100 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
